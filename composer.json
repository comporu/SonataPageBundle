{
    "name": "sonata-project/page-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataPageBundle",
    "keywords": ["page", "sonata", "cms"],
    "homepage": "https://sonata-project.org/bundles/page",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataPageBundle/contributors"
        }
    ],
    "require": {
        "symfony/symfony": "~2.3|~3.0",
        "sonata-project/admin-bundle": "*",
        "sonata-project/block-bundle": "*",
        "sonata-project/seo-bundle": "*",
        "sonata-project/doctrine-extensions" : "*",
        "sonata-project/cache-bundle": "*",
        "sonata-project/cache": "*",
        "sonata-project/core-bundle": "*",
        "symfony-cmf/routing-bundle": "~1.1",
        "sonata-project/notification-bundle": "*",
        "sonata-project/datagrid-bundle": "*",
        "cocur/slugify": "~1.0"
    },
    "require-dev": {
        "sonata-project/doctrine-orm-admin-bundle": "~2.2",
        "symfony/phpunit-bridge": "~2.7|~3.0",
        "friendsofsymfony/rest-bundle": "~1.1",
        "jms/serializer-bundle": "~0.11|~1.0",
        "nelmio/api-doc-bundle": "~2.4"
    },
    "conflict": {
        "jms/serializer": "<0.13"
    },
    "suggest": {
        "sonata-project/doctrine-orm-admin-bundle": "~2.2"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "Sonata\\PageBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4.x-dev"
        }
    }
}
